{{#if productos.length}}
<h2>Lista de productos</h2>
<div>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
    {{#each productos}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.precio}}</td>
            <td><img src="{{this.imagen}}" alt="" style="width: 40px; height: auto"></td>
        </tr>
    {{/each}}
    </table>
</div>
{{/if}}{{#else}}
    <h3>No se encontraron productos</h3>
    {{/else}}